{
  "name": "cf-email-panel",
  "version": "1.0.0",
  "description": "Cloudflare Email Routing Panel",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:ps": "powershell -ExecutionPolicy Bypass -File start.ps1",
    "setup": "node setup.js",
    "deploy:worker": "cd worker && npx wrangler deploy",
    "start:bot": "node bot.js",
    "start:all": "npx concurrently \"npm start\" \"npm run start:bot\""
  },
  "license": "ISC",
"dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "postal-mime": "^2.7.1",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "telegraf": "^4.16.3"
  },
"devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.11"
  }
}
